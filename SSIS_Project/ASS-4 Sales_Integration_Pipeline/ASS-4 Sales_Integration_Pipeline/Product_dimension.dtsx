          SQLTask:Connection="{AAC7A01D-31C6-4D9A-9391-3BB101D38EC0}"
          SQLTask:SqlStatementSource="USE Customers_sales_integration;&#xA;GO&#xA;&#xA;--- ==========================================================&#xA;-- Clean Tables for Product Dimension Package&#xA;-- ==========================================================&#xA;&#xA;-- Step 1: Drop FK constraint for Product&#xA;IF EXISTS (SELECT * FROM sys.foreign_keys WHERE name = 'FK_Sales_Product')&#xA;    ALTER TABLE Fact_Sales_Clean DROP CONSTRAINT FK_Sales_Product;&#xA;&#xA;-- Step 2: Clean fact table and product table&#xA;TRUNCATE TABLE Fact_Sales_Clean;&#xA;DELETE FROM Sales_Rejected;&#xA;DELETE FROM Sales_Daily_Summary;&#xA;TRUNCATE TABLE Dim_Product_master_table;&#xA;&#xA;-- Step 3: Recreate FK constraint for Product&#xA;ALTER TABLE Fact_Sales_Clean&#xA;ADD CONSTRAINT FK_Sales_Product &#xA;    FOREIGN KEY (ProductId) &#xA;    REFERENCES Dim_Product_master_table(ProductId);&#xA;&#xA;-- Step 4: Reset identity seeds&#xA;DBCC CHECKIDENT ('Dim_Product_master_table', RESEED, 0);&#xA;DBCC CHECKIDENT ('Fact_Sales_Clean', RESEED, 0);&#xA;DBCC CHECKIDENT ('Sales_Rejected', RESEED, 0);&#xA;DBCC CHECKIDENT ('Sales_Daily_Summary', RESEED, 0);&#xA;&#xA;PRINT '✓ Product table cleaned';" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
